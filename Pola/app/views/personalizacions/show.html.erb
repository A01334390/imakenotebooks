<%- model_class = Personalizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:liston) %>:</strong></dt>
  <dd><%= @personalizacion.liston %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imagen) %>:</strong></dt>
  <dd><%= @personalizacion.imagen %></dd>
  <dt><strong><%= model_class.human_attribute_name(:resorte) %>:</strong></dt>
  <dd><%= @personalizacion.resorte %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipodehoja) %>:</strong></dt>
  <dd><%= @personalizacion.tipodehoja %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              personalizacions_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_personalizacion_path(@personalizacion), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              personalizacion_path(@personalizacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
